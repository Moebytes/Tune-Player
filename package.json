{
  "name": "tune-player",
  "version": "0.9.9",
  "description": "A simple music player",
  "main": "dist/main/index.js",
  "scripts": {
    "start": "npm run clean && rsbuild build && electron dist/main/index.js",
    "build": "npm run clean ./build && rsbuild build && electron-builder -p never",
    "clean": "del-cli ./dist"
  },
  "keywords": [
    "music",
    "audio",
    "midi",
    "player",
    "anime",
    "cute"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Moebytes/Tune-Player"
  },
  "build": {
    "appId": "com.moebytes.tune-player",
    "productName": "Tune Player",
    "icon": "./assets/icon.png",
    "directories": {
      "buildResources": "assets",
      "output": "build"
    },
    "mac": {
      "target": {
        "target": "dmg",
        "arch": "arm64"
      },
      "category": "public.app-category.music",
      "extraFiles": [
        "structures/bitcrusher.js",
        "structures/lfo.js",
        "structures/soundtouch.js"
      ]
    },
    "dmg": {
      "title": "Tune Player",
      "background": "./assets/background.png",
      "window": {
        "width": 540,
        "height": 380
      }
    },
    "win": {
      "target": {
        "target": "nsis",
        "arch": "x64"
      },
      "extraFiles": [
        "structures/bitcrusher.js",
        "structures/lfo.js",
        "structures/soundtouch.js"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "uninstallDisplayName": "Tune Player",
      "createDesktopShortcut": "always",
      "installerSidebar": "./assets/installer.bmp",
      "uninstallerSidebar": "./assets/installer.bmp"
    },
    "linux": {
      "target": {
        "target": "appImage",
        "arch": "x64"
      },
      "category": "Audio",
      "extraFiles": [
        "structures/bitcrusher.js",
        "structures/lfo.js",
        "structures/soundtouch.js"
      ]
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "publish": {
      "provider": "github",
      "repo": "Tune-Player",
      "owner": "Moebytes",
      "releaseType": "draft"
    },
    "fileAssociations": [
      {
        "ext": "mp3",
        "description": "Audio files"
      },
      {
        "ext": "wav",
        "description": "Audio files"
      },
      {
        "ext": "ogg",
        "description": "Audio files"
      },
      {
        "ext": "flac",
        "description": "Audio files"
      },
      {
        "ext": "aac",
        "description": "Audio files"
      },
      {
        "ext": "mid",
        "description": "MIDI files"
      }
    ]
  },
  "author": "Moebytes",
  "license": "CC BY-NC 4.0",
  "devDependencies": {
    "@electron-rsbuild/plugin-main": "^0.0.12",
    "@electron-rsbuild/plugin-preload": "^0.0.12",
    "@electron-rsbuild/plugin-renderer": "^0.0.12",
    "@rsbuild/core": "^1.7.3",
    "@rsbuild/plugin-less": "^1.6.0",
    "@rsbuild/plugin-node-polyfill": "^1.4.4",
    "@rsbuild/plugin-react": "^1.4.5",
    "@rsbuild/plugin-type-check": "^1.3.3",
    "@svgr/webpack": "^8.1.0",
    "@types/react": "^18.3.28",
    "@types/react-dom": "^18.3.7",
    "cross-env": "^10.1.0",
    "del-cli": "^7.0.0",
    "electron": "^40.4.1",
    "electron-builder": "^26.8.1",
    "node-loader": "^2.1.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@electron/osx-sign": "^2.3.0",
    "@reduxjs/toolkit": "^2.11.2",
    "@tonejs/midi": "^2.0.28",
    "audio-encoder": "^1.0.4",
    "bootstrap": "^5.3.8",
    "browser-id3-writer": "^6.3.1",
    "electron-click-drag-plugin": "^2.0.2",
    "electron-shortcuts": "^0.4.0",
    "electron-store": "^8.0.0",
    "jsmediatags": "^3.9.7",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-slider": "^2.0.6",
    "soundcloud.ts": "^0.7.2",
    "tone": "^15.1.22",
    "web-audio-beat-detector": "^8.2.35",
    "youtube.ts": "^0.3.5"
  }
}
